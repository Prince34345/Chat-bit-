    "lint": "eslint . --ignore-path eslintignore",
    "lint:fix": "eslint . --ignore-path eslintignore --fix",
    "lint:css": "stylelint \"**/*.css\"  --ignore-pattern stylelintignore ",
    "lint:css:fix": "stylelint \"**/*.css\"  --ignore-pattern stylelintignore --fix",
    "prepare": "husky install .husky",
    "format": "prettier --ignore-path prettierignore --write \"**/*.+(jsx|tsx|js|ts|json)\"",
    "test": "jest",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch",
    "serve": "webpack serve"
